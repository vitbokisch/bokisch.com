    "@vitus-labs/tools-typescript": "^0.33.0",
    "image-trace-loader": "^1.0.2",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^9.0.0",
    "next-compose-plugins": "^2.2.1",
    "next-optimized-images": "^2.6.2",
    "next-pwa": "^5.5.4",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "webp-loader": "^0.6.0"
  },
  "packageManager": "yarn@3.2.1"
