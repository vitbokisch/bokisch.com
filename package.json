    "@vitus-labs/rocketstyle": "^0.79.0",
    "@vitus-labs/unistyle": "^0.79.0",
    "mobx": "^6.7.0",
    "mobx-react": "^7.6.0",
    "mobx-state-tree": "^5.1.7",
    "next": "^13.1.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.6"
  },
