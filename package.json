    "image-trace-loader": "^1.0.2",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^10.0.1",
    "next-compose-plugins": "^2.2.1",
    "next-optimized-images-2": "^1.0.1",
    "next-pwa": "^5.6.0",
