  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=3.0.0"
  },
  "scripts": {
    "dev": "next",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "lint:css": "stylelint src/*.ts src/*.tsx",
    "lint:ts": "eslint src/*",
    "lint": "yarn lint:css && yarn lint:ts",
    "pkgs": "rm -rf node_modules/ package-lock.json yarn.lock && yarn",
    "release": "rm -rf node_modules/.cache && next build && next export && touch out/.nojekyll && cp CNAME out/CNAME  && git add -f out/ && git commit -m \"Release new version\" && git subtree push --prefix out origin release",
    "pkgs:clean": "rm -rf node_modules/ yarn.lock && yarn",
    "pkgs:build": "lerna run build --stream",
    "make:favicon": "yarn vl_favicon"
  },
  "dependencies": {
    "@next/font": "^13.1.4",
    "@vitus-labs/coolgrid": "^0.79.0",
