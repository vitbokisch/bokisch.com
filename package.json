{
  "name": "@vitbokisch/personal-web",
  "version": "0.0.0",
  "description": "A personal website",
  "main": "index.js",
  "repository": "https://github.com/vitbokisch/bokisch.com.git",
  "author": "Vit Bokisch <vit@bokisch.cz>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=3.0.0"
  },
  "scripts": {
    "dev": "next",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "lint:css": "stylelint src/*.ts src/*.tsx",
    "lint:ts": "eslint src/*",
    "lint": "yarn lint:css && yarn lint:ts",
    "pkgs": "rm -rf node_modules/ package-lock.json yarn.lock && yarn",
    "release": "rm -rf node_modules/.cache && next build && next export && touch out/.nojekyll && cp CNAME out/CNAME  && git add -f out/ && git commit -m \"Release new version\" && git subtree push --prefix out origin release",
    "pkgs:clean": "rm -rf node_modules/ yarn.lock && yarn",
    "pkgs:build": "lerna run build --stream",
    "make:favicon": "yarn vl_favicon"
  },
  "dependencies": {
    "@vitus-labs/coolgrid": "0.60.0",
    "@vitus-labs/core": "0.60.0",
    "@vitus-labs/elements": "0.60.0",
    "@vitus-labs/hooks": "0.60.0",
    "@vitus-labs/rocketstyle": "0.60.0",
    "@vitus-labs/unistyle": "0.60.0",
    "mobx": "^6.6.0",
    "mobx-react": "^7.5.0",
    "mobx-state-tree": "^5.1.5",
    "next": "^12.1.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.14",
    "@types/styled-components": "^5.1.25",
    "@vitus-labs/tools-babel": "^0.33.0",
